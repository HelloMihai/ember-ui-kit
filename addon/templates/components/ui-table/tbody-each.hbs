{{ui-scrollable class="ui-table__froze" ps=(hash suppressScrollX=true)}}
{{#ui-scrollable class="ui-table__unfroze" ps=(hash minScrollbarLength=40)}}
  {{#each buffer key="key" as |item index|}}
    {{yield
      (hash
        r=(component "ui-table/tr" table=table tbody=this bufferIndex=index itemIndex=item.index)
      )
      item.model
      item.index
    }}
  {{/each}}
{{/ui-scrollable}}
